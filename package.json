{
  "name": "burnabot",
  "version": "1.0.0",
  "description": "bot for Dylan Burns Discord",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xanazf/burnabot.git"
  },
  "keywords": [
    "Discord",
    "bot",
    "streamer",
    "youtuber",
    "content",
    "creator",
    "custom"
  ],
  "author": "Xanazf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xanazf/burnabot/issues"
  },
  "homepage": "https://github.com/Xanazf/burnabot#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.19.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
